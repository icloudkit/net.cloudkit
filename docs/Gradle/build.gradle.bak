apply plugin: 'java'

archivesBaseName = 'cloudkit-commons'

group 'net.cloudkit.commons'
// 指定build输出目录和版本号
version = '1.0'
// 默认为 build
// buildDir = "target"

// MANIFEST.MF 配置信息
jar {
    manifest {
        /*
        MANIFEST.MF
        Manifest-Version: 1.0
        Implementation-Vendor: The Netty Project
        Implementation-Title: Netty/All-in-One
        Implementation-Version: 5.0.0.Alpha2
        Implementation-Vendor-Id: io.netty
        Built-By: norman
        Build-Jdk: 1.7.0_75
        Created-By: Apache Maven 3.2.3
        Archiver-Version: Plexus Archiver
        */
        attributes 'Implementation-Vendor': 'The CloudKit Project', 'Implementation-Title': 'Commons/CloudKit', 'Implementation-Version': version, 'Implementation-Vendor-Id': 'net.cloudkit.commons', 'Built-By': 'norman', 'Build-Jdk': '1.7.0_79', 'Created-By': 'Gradle 2.6', 'Archiver-Version': 'Plexus Archiver'
    }
}

dependencies {

}